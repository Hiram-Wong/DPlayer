        "url-loader": "^4.1.1",
        "vuepress": "1.9.7",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.0",
        "yorkie": "^2.0.0"
    },
    "dependencies": {
        "aribb24.js": "1.9.14",
        "axios": "^0.27.2",
        "balloon-css": "^1.2.0",
        "promise-polyfill": "^8.2.3"
    },
    "gitHooks": {
        "pre-commit": "npm run format:staged"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
