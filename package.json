{
    "name": "dplayer",
    "version": "1.26.0",
    "description": "Wow, such a lovely HTML5 danmaku video player",
    "main": "dist/DPlayer.min.js",
    "files": [
        "dist"
    ],
    "keywords": [
        "player",
        "danmaku",
        "video",
        "html5"
    ],
    "homepage": "https://github.com/tsukumijima/DPlayer",
    "bugs": {
        "url": "https://github.com/tsukumijima/DPlayer/issues"
    },
    "license": "MIT",
    "author": "DIYgod",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tsukumijima/DPlayer.git"
    },
    "scripts": {
        "start": "npm run dev",
        "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js --watch --colors",
        "test": "eslint src webpack",
        "format": "eslint \"**/*.js\" --fix",
        "format:staged": "eslint \"**/*.js\" --fix",
        "format:check": "eslint \"**/*.js\"",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs"
    },
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@vuepress/plugin-back-to-top": "1.9.7",
        "@vuepress/plugin-google-analytics": "1.9.7",
        "@vuepress/plugin-pwa": "1.9.7",
        "art-template": "4.13.2",
        "art-template-loader": "1.4.3",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "cssnano": "^5.1.10",
        "eslint": "^8.16.0",
        "eslint-loader": "^4.0.0",
        "exports-loader": "^4.0.0",
        "file-loader": "^6.2.0",
        "git-revision-webpack-plugin": "^5.0.0",
        "mini-css-extract-plugin": "2.6.0",
        "postcss-loader": "^7.0.0",
        "sass": "^1.52.1",
        "sass-loader": "^13.0.0",
        "strip-loader": "^0.1.2",
